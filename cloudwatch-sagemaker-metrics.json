{
  "metrics": {
    "namespace": "SageMaker",
    "metrics_collected": {
      "cpu": {
        "measurement": [ "cpu_usage_system" ],
        "metrics_collection_interval": 60,
        "append_dimensions": { "notebook_instance_name": "MyNotebookInstance" },
        "resources": [ "*" ],
        "totalcpu": true
      },
      "disk": {
        "measurement": [ "used_percent" ],
        "metrics_collection_interval": 60,
        "append_dimensions": { "notebook_instance_name": "MyNotebookInstance" },
        "resources": [ "*" ]
      },
      "mem": {
        "measurement": [ "mem_used_percent" ],
        "metrics_collection_interval": 60,
        "append_dimensions": { "notebook_instance_name": "MyNotebookInstance" }
      }
    }
  }
}
